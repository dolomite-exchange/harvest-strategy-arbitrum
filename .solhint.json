{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "avoid-suicide": "error",
    "avoid-sha3": "error",
    "func-param-name-mixedcase": "error",
    "modifier-name-mixedcase": "error",
    "max-line-length": ["error", 120],
    "constructor-syntax": "warn",
    "private-vars-leading-underscore": "warn",
    "ordering": "error"
  }
}
